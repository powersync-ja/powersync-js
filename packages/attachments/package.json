{
  "name": "@powersync/attachments",
  "version": "2.4.3",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powersync-ja/powersync-js.git"
  },
  "deprecated": "Attachment support is now built into the PowerSync SDKs. Please import from your platform-specific SDK instead (e.g. @powersync/react-native, @powersync/web, or @powersync/node).",
  "author": "JOURNEYAPPS",
  "license": "Apache-2.0",
  "homepage": "https://docs.powersync.com",
  "bugs": {
    "url": "https://github.com/powersync-ja/powersync-js/issues"
  },
  "description": "An PowerSync library to manage attachments for TypeScript and React Native apps",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "lib",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b && rollup --config",
    "build:prod": "tsc -b --sourceMap false && rollup --config",
    "clean": "rm -rf lib dist tsconfig.tsbuildinfo",
    "watch": "tsc -b -w",
    "test": "pnpm build && vitest",
    "test:exports": "attw --pack ."
  },
  "peerDependencies": {
    "@powersync/common": "workspace:^1.47.0"
  },
  "devDependencies": {
    "@powersync/common": "workspace:*",
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-typescript": "catalog:",
    "@types/node": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-dts": "catalog:",
    "vite": "catalog:"
  }
}
